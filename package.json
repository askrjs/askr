{
  "name": "@askrjs/askr",
  "version": "0.0.5",
  "description": "Actor-backed deterministic UI framework",
  "type": "module",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/askrjs/askr"
  },
  "homepage": "https://github.com/askrjs/askr#readme",
  "bugs": {
    "url": "https://github.com/askrjs/askr/issues"
  },
  "author": {
    "name": "askrjs",
    "url": "https://github.com/askrjs"
  },
  "keywords": [
    "askr",
    "ui",
    "framework",
    "jsx",
    "ssr"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "./dist/jsx/types.js",
    "./dist/runtime/fastlane.js"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./resources": {
      "import": "./dist/resources/index.js",
      "types": "./dist/resources/index.d.ts"
    },
    "./fx": {
      "import": "./dist/fx/index.js",
      "types": "./dist/fx/index.d.ts"
    },
    "./router": {
      "import": "./dist/router/index.js",
      "types": "./dist/router/index.d.ts"
    },
    "./ssr": {
      "import": "./dist/ssr/index.js",
      "types": "./dist/ssr/index.d.ts"
    },
    "./jsx-runtime": {
      "import": "./dist/jsx-runtime.js",
      "types": "./dist/jsx-runtime.d.ts"
    },
    "./jsx-dev-runtime": {
      "import": "./dist/jsx-dev-runtime.js",
      "types": "./dist/jsx-dev-runtime.d.ts"
    },
    "./vite": {
      "import": "./dist/vite/index.js",
      "types": "./dist/vite/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:types": "npm run build && tsd",
    "bench": "cross-env NODE_ENV=production vitest bench --run",
    "fmt": "prettier --write .",
    "lint": "eslint src tests benches --ext .ts,.tsx"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^27.4.0",
    "prettier": "^3.7.4",
    "tsup": "^8.5.1",
    "tsd": "^0.33.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.16",
    "vite": "^7.3.0"
  }
}
